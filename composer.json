{
  "name": "alexain/simple-spreadsheet-reader",
  "description": "A lightweight Symfony bundle to read spreadsheet-like files (CSV, XLSX) using a unified, streaming-friendly API.",
  "type": "symfony-bundle",
  "homepage": "https://github.com/alexain/simple-spreadsheet-reader",
  "license": "MIT",
  "authors": [
    {
      "name": "Alessandro Capano",
      "email": "xainbit@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "symfony/http-kernel": "^7.0|^8.0",
    "symfony/dependency-injection": "^7.0|^8.0",
    "symfony/config": "^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "Alexain\\SimpleSpreadsheetReader\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0-dev"
    },
    "symfony": {
      "requires": "7.0",
      "allow-contrib": "true"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.92"
  }
}
